<aura:component support="BETA" description="Only usable in the body of aura:library (.lib) files. This attribute is used to specifiy the Javascript files that are loaded by the library and which file depend on which.">
    
	<aura:attribute name="name" type="String" required="true" access="GLOBAL" description="Javascript file name in the same folder as the .lib file. The .js extension is omitted."/>
	<aura:attribute name="imports" type="String" required="true" access="GLOBAL" description="Specifies a comma separated list of files in the same folder as the .lib that are required by the Javascript file being included. The required files will be passed in as arguments to the module's function."/>
	
</aura:component>