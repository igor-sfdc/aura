<aura:component>
	<ui:block aura:id="overflowHidden">
		 <aura:set attribute="left">
		 	<div id="leftBlockDiv">Div inside left block</div>
		 </aura:set>
		 <aura:set attribute="right">
		 	<div id="rightBlockDiv">Div inside right block</div>
		 </aura:set>
	 	<div id="bodyBlockDiv">div in block body</div>
	</ui:block>
	<ui:block aura:id="overflowVisible" overflow="true" tag="span">
		<div id="overflowVisibleBody">div in block body</div>
	</ui:block>
</aura:component>