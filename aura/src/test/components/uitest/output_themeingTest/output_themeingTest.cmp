<aura:component>
	<aura:attribute name="item" type="String" default="A long time ago, in a galaxy far, far away... "/>
	<ui:label label="ui:outputCheckbox  checked: " /><ui:outputCheckbox value="true"/>  <br/>
	<ui:label label="ui:outputCheckbox  Not checked: " /><ui:outputCheckbox value="false"/>  <br/>
	<ui:label label="ui:outputURL   Not disabled checked: " /><ui:outputURL  iconClass="iconClassLink" label="temp" value="SomethingSomethingDarkSide"/>  <br/>
	<ui:label label="ui:outputTextArea  Not disabled checked: " /><ui:outputTextArea value="{!v.item}"/>  <br/>
	<ui:label label="ui:outputLabel  Not disabled checked: " /><ui:outputLabel value="THIS IS A LABEL"/>  <br/>
</aura:component>

